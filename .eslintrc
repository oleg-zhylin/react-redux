{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "es6": true
  },
  "globals": {
    "fetch": false,
    "console": false,
    "document": false,
    "window": false,
    "copy": false,
    "process": false
  },
  "rules": {
    "no-console":0,
    "max-len": [1, 160, 4, {"ignoreComments": true}]
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}