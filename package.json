{
  "name": "My-website-v3",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run local",
    "local": "cross-env NODE_ENV=local concurrently --kill-others \"npm run dev:server\" \"npm run webpack:server\"",
    "dev:build": "webpack --config webpack.dev.js --progress",
    "prod:build": "cross-env NODE_ENV=production webpack -p --config webpack.prod.js --progress",
    "webpack:server": "webpack-dev-server --config webpack.local.js",
    "dev:server": "cd ./server && npm start",
    "prod:server": "cd ./server && cross-env NODE_ENV=production npm start",
    "zip": "cd ./server && git archive --format=zip HEAD > My-website-3.0-$(git rev-parse --short HEAD).zip",
    "ia": "npm i && cd ./server && npm i",
    "build": "npm run dev:build && npm run prod:build && npm run zip"
  },
